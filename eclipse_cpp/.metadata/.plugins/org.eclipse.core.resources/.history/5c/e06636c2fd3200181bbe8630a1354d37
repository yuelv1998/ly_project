#include <iostream>
#include<algorithm>
#include<map>
#include<iterator>
#include<string>
using namespace std;
void fun(int &a)
{
   a = a * a;
}

int main() {
	vector<int>  vec;
	for(int i = 0; i < 9; i++)
	{
		vec.push_back(i);
	}

	for_each(vec.begin(), vec.end(), fun);



	/*
